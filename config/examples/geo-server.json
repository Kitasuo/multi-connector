{
  "template": "geo-server",
  "static": {
    "url": "<key-path>"
  },
  "dynamic": {
    "authConfig.path": [
      "idLocal",
      "lowLeftCornerCoordinateAxis1",
      "lowLeftCornerCoordinateAxis2",
      "upperRightCornerCoordinateAxis1",
      "upperRightCornerCoordinateAxis2"
    ]
  },
  "request": {
    "period": "<start>/<end>",
    "targetObject": {
      "idLocal": "",
      "lowLeftCornerCoordinateAxis1": "<dclat>",
      "lowLeftCornerCoordinateAxis2": "<dclon>",
      "upperRightCornerCoordinateAxis1": "<uclat>",
      "upperRightCornerCoordinateAxis2": "<uclon>"
    }
  }
}
