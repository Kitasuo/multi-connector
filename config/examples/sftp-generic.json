{
    "template": "sftp-generic",
    "static": {
        "url": "<host>",
        "port": "<port>",
        "fromPath": "[<from-path>]",
        "toPath": "[<to-path>]",
        "username": "<username>",
        "password": "<password>",
        "proxyHost": "[<socks-server-host>]",
        "proxyPort": "[<socks-server-port>]"
    },
    "dynamic": {
        "authConfig.path": "ids"
    },
    "request": {
        "ids": [
            "<path>"
        ]
    }
}
