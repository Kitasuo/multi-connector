{
    "template": "c4-cals-purchase-order",
    "static": {
        "env": "development",
        "apikey": "<apikey>",
        "bearer": "<bearer>",
        "instanceId": "<instance-id>"
    },
    "dynamic": {
        "authConfig.path": "targetObject.order.idLocal",
        "authConfig.instance": "instanceId"
    },
    "request": {
        "targetObject": {
            "order": {
                "idLocal": "<id-local>"
            }
        }
    }
}
