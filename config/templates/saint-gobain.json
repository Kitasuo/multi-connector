{
  "plugins": ["oauth2"],
  "protocol": "rest",
  "authConfig": {
    "url": "${url}",
    "authPath": "/oauth/token",
    "clientId": "${clientId}",
    "clientSecret": "${clientSecret}",
    "username": "${username}",
    "password": "${password}",
    "path": "${path}"
  },
  "dataObjects": [
    "entity"
  ],
  "dataPropertyMappings": {
    "${type}": ""
  },
  "generalConfig": {
    "hardwareId": {
      "dataObjectProperty": "_id.$oid"
    }
  },
  "input": {
    "required": []
  },
  "output": {
    "contextValue": "https://standards.oftrust.net/v2/Context/DataProductOutput/ProductCatalog/",
    "array": "products",
    "data": "data"
  }
}
