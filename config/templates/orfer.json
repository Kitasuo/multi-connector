{
  "plugins": [
    "azure-service-bus",
    "mqtt-broker",
    "orfer"
  ],
  "protocol": "mqtt",
  "authConfig": {
    "url": "${url}",
    "path": "${id}",
    "topic": "${topic}"
  },
  "dataObjects": [
    ""
  ],
  "dataPropertyMappings": {
    "Process": ""
  },
  "generalConfig": {
    "sourceName": {
      "dataObjectProperty": "@type"
    },
    "hardwareId": {
      "dataObjectProperty": "name"
    },
    "timestamp": {
      "dataObjectProperty": "timestamp"
    }
  },
  "output": {
    "contextValue": "https://standards.oftrust.net/v2/Context/DataProductOutput/Process/",
    "array": "processes",
    "data": "data"
  }
}
