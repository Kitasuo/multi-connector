{
  "plugins": [
    "basic",
    "rakennustieto"
  ],
  "protocol": "rest",
  "authConfig": {
    "url": "${url}",
    "username": "${username}",
    "password": "${password}",
    "path": "/ws/rest/rakennustieto/api/tradeItems/${property}/${value}"
  },
  "dataObjects": [
    "Items"
  ],
  "dataPropertyMappings": {
    "${type}": ""
  },
  "generalConfig": {
    "sourceName": {
      "dataObjectProperty": "ItemTechnicalName"
    },
    "hardwareId": {
      "dataObjectProperty": "${property}"
    },
    "timestamp": {
      "dataObjectProperty": "ItemModifiedAt"
    }
  },
  "pot": {
    "context": "https://standards.oftrust.net/v2/Context/DataProductParameters/Product/",
    "array": "products",
    "data": "data"
  }
}
