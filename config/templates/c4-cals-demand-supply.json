{
  "plugins": [
    "c4-cals"
  ],
  "protocol": "rest",
  "authConfig": {
    "url": "https://c4-prod-apim.azure-api.net/pot",
    "headers": {
      "CALS-API-KEY": "${apikey}"
    },
    "path": "/instances/${instance}/demandsupplies/${targetObject.VendorId}"
  },
  "dataObjects": [
    ""
  ],
  "dataPropertyMappings": {
    "${type}": ""
  },
  "generalConfig": {
    "sourceName": {
      "dataObjectProperty": "VendorName"
    },
    "hardwareId": {
      "dataObjectProperty": "VendorId"
    }
  },
    "input": {
        "required": [
            "parameters.targetObject"
        ]
    },
  "output": {
    "contextValue": "https://standards.oftrust.net/v2/Context/DataProductOutput/DemandSupply/",
    "array": "demandSupply",
    "data": "data"
  }
}
