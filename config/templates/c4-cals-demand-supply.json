{
  "plugins": [
    "c4-cals",
    "broker"
  ],
  "protocol": "custom",
  "authConfig": {
    "url": "https://c4-prod-apim.azure-api.net/pot",
    "headers": {
      "CALS-API-KEY": "${apikey}"
    },
    "instance": "${instance}",
    "path": "/instances/${instance}/demandsupplies/${targetObject.vendor.idLocal}"
  },
  "dataObjects": [
    ""
  ],
  "dataPropertyMappings": {
    "DemandInformation": ""
  },
  "generalConfig": {
    "sourceName": {
      "dataObjectProperty": "vendorName"
    },
    "hardwareId": {
      "dataObjectProperty": "vendorId"
    }
  },
  "input": {
    "required": [
      "parameters.targetObject"
    ]
  },
  "output": {
    "contextValue": "https://standards.oftrust.net/v2/Context/DataProductOutput/DemandInformation/",
    "array": "demand",
    "data": "data"
  }
}
