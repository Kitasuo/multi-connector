{
  "plugins": [
    "oauth2"
  ],
  "protocol": "rest",
  "authConfig": {
    "url": "${authUrl}",
    "authPath": "/token",
    "clientId": "${clientId}",
    "clientSecret": "${clientSecret}",
    "scope": "openid",
    "path": "${dataUrl}/v2/iaq/sensors/latest/${id}"
  },
  "dataObjects": [
    "data"
  ],
  "dataPropertyMappings": {
    "MeasureAirTemperatureCelsiusDegree": "Temperature_01",
    "MeasureAirHumidityPercentage": "Humidity_01"
  },
  "generalConfig": {
    "hardwareId": {
      "pathIndex": 7
    },
    "timestamp": {
      "dataObjectProperty": "timestamp"
    }
  }
}
